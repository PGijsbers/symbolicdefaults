{
    "name": "mlr_rf",
    "experiment_data": "data/rbv2_mlr_classif.ranger.arff",
    "metric": "perf.mmce",
    "how": "minimize",
    "metadata": "data/after_pipeline_metafeatures_v2.csv",
    "surrogates": "data/ranger_surrogates.pkl",
    "hyperparameters": [
      "num.trees",
      "sample.fraction",
      "mtry",
      "min.node.size"
    ],
    "fixed": {
      "num.trees": 500.0
    },
    "filters": {
      "splitrule": "gini"
    },
    "operators": "default",
    "performance_column": "perf.mmce",
    "ignore": [
      "perf.logloss",
      "learner",
      "traintime",
      "predicttime",
      "dataset"
    ],
    "benchmark": {
      "mlr_default": "make_tuple(100., 1., 10., 1.)"
    }
}
