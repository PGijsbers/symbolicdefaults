{
    "name": "mlr_xgboost",
    "experiment_data": "data/rbv2_mlr_classif.xgboost.arff",
    "metric": "perf.mmce",
    "how": "minimize",
    "metadata": "data/after_pipeline_metafeatures_v2.csv",
    "surrogates": "data/xgboost_surrogates.pkl",
    "hyperparameters": {
      "nrounds": "int",
      "eta": "float",
      "lambda": "float",
      "gamma": "float",
      "alpha": "float",
      "subsample": "float",
      "max_depth": "int",
      "min_child_weight": "float",
      "colsample_bytree": "float",
      "colsample_bylevel": "float"
    },
    "fixed": {"nrounds": 500.0},
    "filters": {"booster" : "gbtree"},
    "operators": "default",
    "performance_column": "perf.mmce",
    "ignore": [
      "perf.logloss",
      "learner",
      "traintime",
      "predicttime",
      "dataset"
    ],
    "benchmark": {
      "sklearn_default": "make_tuple(0.1, 1.0, 0.0, 0.0, 1.0, 3.0, 1.0,                  1.0,                                         1.0)",
      "symbolic_best":   "make_tuple(mcp, 2.0, mkd, rc,  n,   5.0, add(expit(xvar), rc), if_gt(0.006719436963715575, xvar, rc, xvar), expit(0.20443125043321808))"
    }
}
